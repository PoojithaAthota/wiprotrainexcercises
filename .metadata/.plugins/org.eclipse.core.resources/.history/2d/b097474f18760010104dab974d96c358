package com.wipro.basic;

public class Loops {

	public static void main(String[] args) {
		for(int i=0;i<10;i++) {
			System.out.println(i);
		}
		
		System.out.println("Reverse");
		
		for(int j=10;j>0;j--) {
			System.out.println(j);
		}
//		for(int i=0;i<10;i=i+2) {
//			System.out.println(i);
//		}


	}

}
